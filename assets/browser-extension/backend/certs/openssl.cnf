[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
x509_extensions = v3_req

[dn]
C=US
ST=State
L=City
O=MCP Platform
OU=Development
CN=192.168.100.10

[v3_req]
subjectAltName = @alt_names

[alt_names]
IP.1 = 192.168.100.10
IP.2 = 127.0.0.1
DNS.1 = localhost
DNS.2 = mcp-server.local
